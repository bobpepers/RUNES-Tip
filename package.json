{
  "name": "node-redux-auth",
  "version": "1.0.0",
  "description": "Token authentication system using Node, Mongo, React, Redux",
  "scripts": {
    "dev": "nodemon --exec babel-node --max-old-space-size=8192 src/index.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "start": "NODE_ENV=production PORT=3333 pm2 start dist/index.js",
    "lint": "eslint src",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "React",
    "Redux",
    "Token Authentication",
    "Airbnb Eslint",
    "SCSS",
    "Babel",
    "Webpack Configuration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimitriMikadze/node-redux-auth"
  },
  "author": "Dimtiri Mikadze",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "bitcoin-core": "^3.0.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "dotenv": "^9.0.2",
    "express": "^4.13.4",
    "lodash": "^4.17.21",
    "metaget": "^1.0.7",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.0",
    "p-queue": "^6.6.2",
    "qr-image": "^3.2.0",
    "qrcode": "^1.4.4",
    "runebaseinfo-lib": "^0.0.1",
    "runebaseinfo-p2p": "^0.0.1",
    "runebaseinfo-rpc": "^0.0.1",
    "rweb3": "git://github.com/runebase/rweb3.js.git#v1.2.5",
    "sequelize": "^6.6.2",
    "telegraf": "^4.3.0",
    "telegraf-ratelimit": "^2.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
